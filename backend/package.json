{
  "name": "backend",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --env-file=.env server.js",
    "test": "dotenv -e .env.test ava --timeout=3m",
    "test:coverage": "dotenv -e .env.test nyc ava --timeout=3m"
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ],
    "extension": [
      ".js"
    ],
    "include": [
      "controllers/**/*.js",
      "models/**/*.js"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/test/**"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "ava": "^6.1.3",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2",
    "firebase": "^10.12.4",
    "firebase-admin": "^12.2.0",
    "js": "^0.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongodb": "^6.8.0",
    "mongodb-memory-server": "^10.0.0",
    "mongoose": "^8.4.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.14",
    "nodemon": "^3.1.4",
    "react-tooltip": "^5.27.1",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "nyc": "^17.0.0"
  }
}
